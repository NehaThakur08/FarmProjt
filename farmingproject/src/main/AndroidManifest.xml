<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neha.farmingproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MyNavActivity"
            android:label="@string/title_activity_my_nav"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".BottomNavActivity"
            android:label="@string/title_activity_bottom_nav" />
        <activity android:name=".ui.otpActivity" />
        <activity android:name=".HomeDashboardActivity" />
        <activity android:name=".ui.LanguageActivity" />
        <activity android:name=".ui.RegistrationActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>